<view class="content"><view class="cell-group"><view class="cell-item right-img"><view class="cell-item-hd"><view class="cell-hd-title color-6">类型筛选</view></view><view class="cell-item-bd"><view class="uni-list"><view class="uni-list-cell-db color-6"><picker value="{{index}}" range="{{objectType}}" data-event-opts="{{[['change',[['changeState',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{objectType[index]}}</view></picker></view><image class="right-img icon" src="/static/image/ic-pull-down.png"></image></view></view></view></view><block wx:if="{{list.length}}"><view class="type-c"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cell-group margin-cell-group"><view class="cell-item"><view class="cell-item-hd"><view class="cell-hd-title">{{item.type}}</view></view><view class="cell-item-ft"><view class="cell-ft-p color-9">{{''+item.ctime+''}}</view></view></view><view class="cell-item"><view class="cell-item-hd"><view class="cell-hd-title color-9">{{"提现卡号："+item.card_number}}</view></view><view class="cell-item-ft red-price">{{''+item.money+''}}</view></view></view></block><uni-load-more vue-id="1" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></view></block><block wx:else><view class="order-none"><image class="cash-none-img" src="/static/image/order.png" mode></image></view></block></view>