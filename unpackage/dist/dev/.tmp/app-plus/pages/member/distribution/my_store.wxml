<view class="content my-store"><view data-ref="myStore" class="vue-ref"><view class="my-store-t"><view class="mst-top"><image src="{{store_banner_src}}" mode="widthFix"></image></view><view class="mst-bot"><view class="member-grid"><view class="member-item"><image class="member-item-img" src="{{store_logo_src}}"></image></view><view class="member-item"><view class="color-o fsz36">{{total_goods}}</view><text class="member-item-text">全部宝贝</text></view><view data-event-opts="{{[['tap',[['openPopup']]]]}}" class="member-item" bindtap="__e"><image class="member-item-icon" src="/static/image/ic-me-collect.png"></image><text class="member-item-text">收藏本店</text></view><view class="member-item"><button data-event-opts="{{[['tap',[['createPoster']]]]}}" class="share btn" bindtap="__e"><image class="member-item-icon" src="/static/image/qr_code.png"></image><text class="member-item-text">二维码</text></button></view></view></view></view><view class="my-store-m"><view class="search"><view data-event-opts="{{[['tap',[['goSearch',['$event']]]]]}}" class="search-c" bindtap="__e"><view class="search-input search-input-p"><view class="search-input-p-c">{{''+searchKey+''}}</view></view><image class="icon search-icon" src="/static/image/zoom.png"></image></view></view></view><lvv-popup class="vue-ref" vue-id="1" position="bottom" data-ref="lvvpopref" data-event-opts="{{[['^click',[['closePopup']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['closePopup']]]]}}" class="collect-pop" bindtap="__e"><block wx:if="{{isWeixinBrowser}}"><image src="/static/image/wxh5.png" mode="widthFix"></image></block></view></lvv-popup></view><view class="img-grids"><block wx:if="{{goodsList.length>0}}"><view><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['goodsDetail',['$0'],[[['goodsList','',index,'id']]]]]]]}}" class="img-grids-item" bindtap="__e"><image class="img-grids-item-t have-none" src="{{item.image_url}}" mode="aspectFill"></image><view class="img-grids-item-b"><view class="goods-name grids-goods-name">{{''+item.name+''}}</view><view class="goods-item-c"><view class="goods-price red-price">{{"￥"+item.price}}</view><image class="goods-cart" src="/static/image/ic-car.png"></image></view></view></view></block></view></block><block wx:else><view class="order-none"><image class="order-none-img" src="/static/image/order.png" mode></image></view></block></view><uni-load-more vue-id="2" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></view>