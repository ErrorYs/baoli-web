<block wx:if="{{cartData.list&&cartData.list.length>0}}"><view class="content"><view class="content-top"><view class="cell-group margin-cell-group"><view class="cell-item"><view class="cell-item-hd"><image class="cell-hd-icon" style="width:32rpx;height:32rpx;" src="/static/image/homepage.png"></image></view><view class="cell-item-bd"><text class="cell-bd-text">{{shopName}}</text></view><view class="cell-item-ft"><block wx:if="{{!editStatus}}"><text data-event-opts="{{[['tap',[['editBtn',['$event']]]]]}}" class="cell-bd-text" bindtap="__e">编辑</text></block><block wx:else><text data-event-opts="{{[['tap',[['editNoBtn',['$event']]]]]}}" class="cell-bd-text" bindtap="__e">完成</text></block></view></view></view><view class="img-list cart-list"><block wx:for="{{cartData.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><checkbox-group class="cart-checkbox" val="{{item.id}}" data-event-opts="{{[['change',[['checkboxChange',['$0'],[[['cartData.list','',index,'id']]]]]]]}}" bindchange="__e"><view><label class="uni-list-cell uni-list-cell-pd"><view class="cart-checkbox-c"><block wx:if="{{item.stockNo}}"><checkbox class="checkboxNo" color="#FF7159" checked="{{item.is_select}}" value="{{item.id}}" disabled="{{item.stockNo}}"></checkbox></block><block wx:else><checkbox color="#FF7159" checked="{{item.is_select}}" value="{{item.id}}"></checkbox></block></view></label><view class="img-list-item"><image class="img-list-item-l little-img have-none" src="{{item.products.image_path}}" mode="aspectFill"></image><view class="img-list-item-r little-right"><view class="little-right-t"><view data-event-opts="{{[['tap',[['goodsDetail',['$0'],[[['cartData.list','',index,'products.goods_id']]]]]]]}}" class="goods-name list-goods-name" bindtap="__e">{{''+item.products.name+''}}</view><view class="goods-price red-price">{{'￥'+item.products.price+''}}</view></view><block wx:if="{{item.products.promotion_list}}"><view class="romotion-tip"><block wx:for="{{item.products.promotion_list}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view class="{{['romotion-tip-item',v.type!==2?'bg-gray':'']}}">{{''+v.name+''}}</view></block></view></block><view class="goods-item-c"><view class="goods-buy"><block wx:if="{{item.products.spes_desc}}"><view class="goods-salesvolume">{{''+item.products.spes_desc+''}}</view></block><block wx:else><view class="goods-salesvolume"></view></block><view class="goods-numbox"><block wx:if="{{item.stockNo&&!editStatus}}"><text class="stockError">库存不足</text></block><block wx:else><block wx:if="{{item.stockTension&&!editStatus}}"><text class="stockError stockTension">库存紧张</text></block></block><block wx:if="{{!editStatus}}"><uni-number-box vue-id="{{'1-'+index}}" min="{{1}}" max="{{item.maxStock}}" value="{{item.nums}}" data-event-opts="{{[['^change',[['bindChange',['$event','$0'],[[['cartData.list','',index]]]]]]]}}" bind:change="__e" bind:__l="__l"></uni-number-box></block><block wx:else><view data-event-opts="{{[['tap',[['del',[index,'$0'],[[['cartData.list','',index,'id']]]]]]]}}" class="click-del" bindtap="__e"><image class="icon" src="/static/image/delete.png" mode></image></view></block></view></view></view></view></view></view></checkbox-group></block></view></view><view class="cart-bottom"><checkbox-group data-event-opts="{{[['change',[['checkboxAllButton',['$event']]]]]}}" class="cart-checkbox" bindchange="__e"><label class="uni-list-cell uni-list-cell-pd"><view class="cart-checkbox-c"><checkbox checked="{{checkboxAll}}" color="#FF7159"></checkbox>全选</view></label><view class="cart-bottom-right"><block wx:if="{{!editStatus}}"><view class="cart-bottom-right-t">合计：<view class="goods-price red-price">{{"￥"+cartData.amount}}</view></view></block><block wx:if="{{!editStatus}}"><button class="btn btn-square btn-b" hover-class="btn-hover2" data-event-opts="{{[['tap',[['settlement',['$event']]]]]}}" bindtap="__e">去结算</button></block><block wx:else><view><button data-event-opts="{{[['tap',[['delList',['$event']]]]]}}" class="btn btn-square btn-b" bindtap="__e">删除</button></view></block></view></checkbox-group></view></view></block><block wx:else><view class="cart-none"><image class="cart-none-img" src="/static/image/car.png" mode></image><view class="cart-none-t">购物车快饿瘪了 T.T</view><view class="cart-none-m">快给我挑点宝贝吧</view><navigator class="cart-none-b" url="../../index/index" hover-class="btn-hover" open-type="switchTab">去逛逛</navigator></view></block>