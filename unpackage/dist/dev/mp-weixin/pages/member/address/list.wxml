<view class="content"><block wx:if="{{list.length}}"><view class="content-top"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="key" wx:key="key"><view class="uni-list-cell uni-list-cell-pd"><view class="cell-group min-cell-group"><view class="cell-item"><view data-event-opts="{{[['tap',[['isSelect',['$0'],[[['list','',key]]]]]]]}}" class="cell-item-hd" bindtap="__e"><view class="cell-hd-title">{{item.name+''}}<text class="phone-num">{{item.mobile}}</text></view></view><view hidden="{{!(type!='order')}}" class="cell-item-ft"><image class="cell-ft-next icon" src="/static/image/compile.png" data-event-opts="{{[['tap',[['toEdit',['$0'],[[['list','',key,'id']]]]]]]}}" bindtap="__e"></image><text class="cell-ft-text"></text></view></view><view data-event-opts="{{[['tap',[['isSelect',['$0'],[[['list','',key]]]]]]]}}" class="cell-item" bindtap="__e"><view class="cell-item-bd"><view class="cell-bd-view"><view hidden="{{!(item.is_def===1)}}" class="cell-tip">默认</view><text class="cell-bd-text">{{item.areaName+item.address}}</text></view></view></view></view></view></block></view></block><block wx:else><view class="address-none"><image class="address-none-img" src="/static/image/order.png" mode></image></view></block><view class="button-bottom"><button class="btn btn-square btn-b" hover-class="btn-hover2" data-event-opts="{{[['tap',[['wechatAddress',['$event']]]]]}}" bindtap="__e">从微信获取</button><button class="btn btn-square btn-w" hover-class="btn-hover2" data-event-opts="{{[['tap',[['toAdd']]]]}}" bindtap="__e">新增收货地址</button></view></view>