.invoice-type .uni-list-cell {
		display: inline-block;
		font-size: 26rpx;
		color: #333;
		position: relative;
		margin-left: 50rpx;
}
.invoice-type .uni-list-cell>view {
		display: inline-block;
}
.invoice-type-icon {
		position: absolute;
		top: 50%;
		-webkit-transform: translateY(-50%);
		        transform: translateY(-50%);
}
.invoice-type-c {
		margin-left: 50rpx;
		line-height: 2;
}
.cell-item-ft .cell-bd-input {
		text-align: right;
		width: 500rpx;
}
.button-bottom .btn {
		width: 100%;
}
.cheque {
		/* position: relative; */
		overflow: visible;
		/* margin-left: 0; */
		/* width: 376px; */
}
.cheque-content {
		position: absolute;
		left: -10rpx;
		top: 90rpx;
		z-index: 2;
		width: 100%;
		/* height: 200rpx; */
		background-color: #fff;
		box-shadow: 0 0 0.666667vw 0.4vw rgba(0, 0, 0, .13);
		border-radius: 10rpx;
		padding: 20rpx;
}
.tips-item {
		margin-bottom: 20rpx;
}
.tips-name {
		font-size: 32rpx;
		line-height: 35rpx;
		color: #333;
		overflow: hidden;
		text-overflow: ellipsis;
		white-space: nowrap;
		margin-bottom: 10rpx;
}
.sub-div {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		width: 100%;
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
		height: 20rpx;
		font-size: 24rpx;
		line-height: 20rpx;
		color: #999;
}
.num {
		display: inline-block;
}

