<view><view hidden="{{!(pickerShow)}}" class="picker-mask" catchtouchmove="true" data-event-opts="{{[['tap',[['closePicker',['$event']]]]]}}" bindtap="__e"></view><view class="{{['picker-content',(pickerShow)?'pickerShow':'']}}"><view class="picker-button"><text data-event-opts="{{[['tap',[['closePicker',['$event']]]]]}}" bindtap="__e">取消</text><text data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" bindtap="__e">确定</text></view><picker-view class="picker-view" indicator-class="picker-view-selected-three" value="{{pickerIndex}}" data-event-opts="{{[['change',[['pickerViewChangeThree',['$event']]]]]}}" bindchange="__e"><picker-view-column><block wx:for="{{pickerList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="picker-item">{{item.label}}</view></block></picker-view-column><picker-view-column><block wx:for="{{pickerList[pickerIndex[0]].children}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="picker-item">{{item.label}}</view></block></picker-view-column><picker-view-column><block wx:for="{{pickerList[pickerIndex[0]].children[pickerIndex[1]].children}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="picker-item">{{item.label}}</view></block></picker-view-column></picker-view></view></view>